{
  "output": {
    // "predict": "Sales.SUM(item_cnt_day|CMONTH(date),shop,item)",
    "pivots": ["CMONTH(date)","shop","item"],
    "pointers": {
      "item": "Items.id",
      "shop": "Shops.id",
      // "__DATE__": "__DATE__",
    },

    "date_block": "CMONTH(date)",
    "date_start": "2013-01",
    "date_end": "2015-10",
  },

  "pivots": {
    "Items": ["id"],
    "Categories": ["id"],
    "Shops": ["id"],
    "Sales": ["item", "shop", "date"],
  },

  "pointers": {
    // "Output.shop": "Shops.id",
    // "Output.item": "Items.id",

    "Items.category": "Categories.id",
  },

  "features": [
    "shop.city_code",
    "item.category",
    "item.category.type_code",
    "item.category.subtype_code",

    "Sales.SUM(item_cnt_day|CMONTH(date),shop,item)", // to predict

    "FWD(Sales.SUM(item_cnt_day|CMONTH(date),shop,item),1,CMONTH(date))", // LAG SHOULD BE 1,2,3,6,12

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date)),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),item),1,CMONTH(date))", // LAG SHOULD BE 1,2,3,6,12

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),shop),1,CMONTH(date))", // LAG SHOULD BE 1,2,3,6,12

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),item.category),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),shop,item.category),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),shop,item.category.type_code),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),shop,item.category.subtype_code),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),shop.city_code),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),item,shop.city_code),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),item.category.type_code),1,CMONTH(date))",

    "FWD(MEAN(Sales.SUM(item_cnt_day|CMONTH(date),shop,item)|CMONTH(date),item.category.subtype_code),1,CMONTH(date))",

    "FWD(Sales.MEAN_DIFF(MEAN(item_cnt_day|item,CMONTH(date))),1,CMONTH(date))",
    // "FWD(Sales.MEAN_DIFF(SUM(revenue|shop,CMONTH(date))),1,CMONTH(date)))",
    // "Sales.MEAN(item_cnt_day|item,CMONTH(date))",
    // "Sales.SUM(revenue|shop,CMONTH(date))",
  ]
}
